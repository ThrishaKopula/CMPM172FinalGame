		{
			"character": "character-1681086055.json",
			"event_id": "dialogic_001",
			"portrait": "",
			"text": "I’ve got the [b]Ballerina[/b]."
		},
		{
			"animation": "[Default]",
			"animation_length": 0,
			"change_mirror_portrait": false,
			"change_z_index": false,
			"character": "character-1681338302.json",
